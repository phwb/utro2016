.navbar
  .navbar-inner
    .left
      a.link.back.icon-only(href='#'): i.icon.icon-back
    .center.sliding Площадки
    .right
      a.link.icon-only.open-panel(href='#'): i.icon.icon-bars
    .subnavbar
      .buttons-row
        a.button.tab-link.active(href='#place-desc') Описание
        a.button.tab-link(href='#place-schedule') Расписание
        a.button.tab-link(href='#place-contacts') Контакты
.pages
  .page.with-subnavbar(data-page='place')
    .tabs
      .page-content.tab.active(id='place-desc')
        .content-block
      .page-content.tab(id='place-schedule')
        .list-block
      .page-content.tab(id='place-contacts')
        .list-block

