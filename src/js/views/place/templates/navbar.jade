.navbar-inner
  .left
    a.link.icon-only.back(href='#'): i.icon.icon-back
  .center.sliding <%= name %>
  .right
    a.link.icon-only.open-panel(href='#'): i.icon.icon-bars
  .subnavbar
    .buttons-row
      a.button.tab-link.active(href!='#place-desc-<%= id %>') Описание
      a.button.tab-link(href!='#place-schedule-<%= id %>') Расписание
      a.button.tab-link(href!='#place-contacts-<%= id %>') Контакты
